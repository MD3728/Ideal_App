      <Rectangle x:Name="PlanTimeWrapper2" RadiusX="8" RadiusY="8" Grid.Column="4" Grid.Row="1" Grid.ColumnSpan="3" Grid.RowSpan="9" HorizontalAlignment="Left" Height="370" VerticalAlignment="Top" Width="150" Fill="#EE7bd0c8" Margin="27,10,10,10" Panel.ZIndex="0" />
