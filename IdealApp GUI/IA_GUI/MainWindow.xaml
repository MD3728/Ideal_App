        <CheckBox x:Name="day5" FontSize="18" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Column="0" Grid.Row="5" Grid.ColumnSpan ="2" Grid.RowSpan="1">
          <Label Margin="0,-5,0,0" Content="Friday" FontSize="18" FontFamily="Arial"/>
