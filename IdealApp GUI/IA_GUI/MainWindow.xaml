      <Label x:Name ="appPathAllowLabel" Panel.ZIndex="2" Margin ="0,-25,0,0" Content="Path Whitelist:" Grid.Column="3" Grid.Row="3" Grid.ColumnSpan ="1" Grid.RowSpan="1" FontSize="18" HorizontalAlignment="Center" VerticalAlignment="Top" />
