      <Label x:Name ="pomoDurationLabel" Content="Work&#x0a;Duration" Margin="10,0,0,0" Grid.Column="0" Grid.Row="6" Grid.ColumnSpan ="2" Grid.RowSpan="1" FontSize="18" HorizontalAlignment="Center" VerticalAlignment="Center"  />
